<!--Displays flow diagram component with given Sprint and Project-->

@inherits BaseProjectScopedComponent

@using Models.Entities
@using ScrumBoard.Shared.Chart
@using ScrumBoard.Shared.Widgets

<div class="row">   
    <div class="col">
        <FlowDiagram Sprint="Sprint" Project="Project"/>
    </div>        
</div> 

@code {
    [CascadingParameter(Name = "Sprint")]
    public Sprint Sprint { get; set; }
}
